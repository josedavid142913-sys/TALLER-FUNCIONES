# PROGRAMA PARA IDENTIFICAR LA TASA DE VALOR

def tomar_dolares():
    dolares = float(input("Digite la cantidad de d√≥lares: "))
    return dolares

def capturar_tasa():
    tasa = 0.86
    return tasa

def analizar_datos(dolares, tasa):
    euros = dolares * tasa
    return euros

def mostrar_resultados(euros):
    print("La cantidad en euros es:", euros)


# Codigo

dolares = tomar_dolares()
tasa = capturar_tasa()
euros = analizar_datos(dolares, tasa)
mostrar_resultados(euros)
