# PROGRAMA PARA HALLAR EL COCIENTE DE LA DIVISION

def pedir_dato1():
    num_uno = int (input("ingrese el numero dividendo: "))
    return num_uno

def pedir_dato2():
    num_dos = int (input("ingrese el numero divisor: "))
    return num_dos

def procesar_informacion(num_uno, num_dos):
    div = num_uno / num_dos
    return div

def resultado(div):
    print ("el cociente de la division es: ", div)
    
# codigo

dato_uno = pedir_dato1()
dato_dos = pedir_dato2()
divv = procesar_informacion(dato_uno, dato_dos)
total = resultado (divv)
